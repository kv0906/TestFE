{
  "extends": ["standard", "plugin:react/recommended"],
  "env":{
    "browser":true,
    "commonjs":true,
    "es6":true
  },
  "plugins":[
    "react"
  ],
  "rules":{
    "indent": ["error"],
    "quote-props": ["error", "as-needed"]
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  }
}